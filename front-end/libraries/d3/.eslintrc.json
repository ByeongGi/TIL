{
  "extends": "airbnb-base",
  "installedESLint": true,
  "env": {
    "browser": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "always",
      "imports": "never",
      "exports": "never",
      "functions": "ignore"
    }],
    "template-curly-spacing": ["error", "always"]
  }
}

